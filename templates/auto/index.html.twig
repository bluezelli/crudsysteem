{% extends 'base.html.twig' %}

{% block title %}Hello AutoController!{% endblock %}

{% block body %}

    <div class="container">
        <div class="row">
            <div class="col">

        <table class="table">
            <thead>
            <tr>
                <th scope="col">Number</th>
                <th scope="col">title</th>
                <th scope="col"></th>
            </tr>
            </thead>
            <tbody>
                {% for autos in auto %}
                    <tr>
                        <td>
                            {{ autos.model }}
                        </td>

                        <td>{{ auto.type }}</td>
                    </tr>

                {% endfor %}
            </tbody>
        </table>
            </div>
        </div>
    </div>



{% endblock %}
